{
  "name": "leetcode-assistant",
  "version": "1.0.0",
  "description": "An AI-powered LeetCode problem solver assistant",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"electron .\"",
    "build": "electron-builder build -w",
    "start": "electron ."
  },
  "build": {
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        }
      ]
    }
  },
  "keywords": [
    "leetcode",
    "interview",
    "coding",
    "assistant"
  ],
  "type": "module",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.14",
    "@ai-sdk/openai": "^1.2.0",
    "@anthropic-ai/sdk": "^0.18.0",
    "@tailwindcss/postcss": "^4.0.9",
    "@tailwindcss/vite": "^4.0.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "ai": "^4.1.51",
    "ffi-napi": "^4.0.3",
    "keyboardjs": "^2.7.0",
    "marked": "^15.0.7",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.0.1",
    "ref-napi": "^3.0.3",
    "screenshot-desktop": "^1.15.0",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "postcss-loader": "^8.1.1",
    "tailwindcss": "^4.0.9"
  }
}
